<div class="cards-container mb-5">
  <div class="cards">
    <% @tasks_per_day.each do |due_date, tasks|  %>
      <h5>Le <%= l(due_date, format: :long) %></h5>
      <% tasks.each do |task| %>
        <%= render "task_card_stimulus", task: task, due_date: due_date %>
      <% end %>
    <% end %>
  </div>
</div>
